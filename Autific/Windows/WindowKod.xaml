<Window x:Class="Autific.Windows.WindowKod"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Autific.Windows"
        mc:Ignorable="d"
        Title="КОД" Height="220" Width="260">
    <Grid>
        <StackPanel HorizontalAlignment="Center">
            <TextBlock Margin="0,10,0,0">Введите числовой код:</TextBlock>
            <TextBox PreviewTextInput="textboxKod_PreviewTextInput" TextChanged="textboxKod_TextChanged" Margin="0,10,0,0" Name="textboxKod"></TextBox>
            <TextBlock Margin="0,10,0,0" Name="textTime"></TextBlock>
            <Button Margin="0,10,0,0" Name="buttinInput" Click="buttinInput_Click">Ввод</Button>
        </StackPanel>
        
    </Grid>
</Window>
